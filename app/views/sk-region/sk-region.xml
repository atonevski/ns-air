<Page loaded='pageLoaded'>
  <ScrollView>
    <StackLayout>
      <Label text="{{ 'Parameter: ' + parameter }}" />
      <Label text="{{ 'Stations: ' + stations }}" />
      <Repeater items='{{ measurements }}'>
        <Repeater.itemTemplate>
          <GridLayout columns='100'>
            <Label text="{{ time }}" class='datetime'/>
          </GridLayout>
        </Repeater.itemTemplate>
      </Repeater>
    </StackLayout>
  </ScrollView>
</Page>
